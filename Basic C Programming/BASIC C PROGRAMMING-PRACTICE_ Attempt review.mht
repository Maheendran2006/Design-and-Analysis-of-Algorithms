    int n;
    int first = 0, second = 1, next;

    scanf("%d", &amp;n);

    if (n == 0)
        printf("%d", first);
    else if (n == 1)
        printf("%d", second);
    else {
        for (int i = 2; i &lt;= n; i++) {
            next = first + second;
            first = second;
            second = next;
        }
        printf("%d", second);
    }

    return 0;
}</textarea><div id="id_q224937:12_answer_wrapper" class="ui_wrapper" style="resize: vertical; overflow: hidden; min-height: 456px; width: 100%; border: 1px solid darkgrey;"><div class=" ace_editor ace_hidpi ace-tomorrow-night ace_dark" style="resize: none; height: 440px; width: 289px;"><textarea class="ace_text-input" wrap="off" autocorrect="off" autocapitalize="off" spellcheck="false" readonly="" style="opacity: 0; height: 1px; width: 1px; transform: translate(55px, 18px);" id="ace_id_q224937:12_answer"></textarea><div class="ace_gutter moodle-has-zindex" aria-hidden="true" style="left: 0px; width: 51px;"><div class="ace_layer ace_gutter-layer ace_folding-enabled" style="height: 1e+06px; transform: translate(0px, 0px); width: 51px;"><div class="ace_gutter-cell ace_gutter-active-line " style="height: 18px; top: 0px;">1<span style="display: none;"></span></div><div class="ace_gutter-cell " style="height: 18px; top: 18px;">2<span style="display: none;"></span></div><div class="ace_gutter-cell " style="height: 18px; top: 36px;">3<span style="display: inline-block; height: 18px;" class="ace_fold-widget ace_start ace_open"></span></div><div class="ace_gutter-cell " style="height: 18px; top: 54px;">4<span style="display: none;"></span></div><div class="ace_gutter-cell " style="height: 18px; top: 72px;">5<span style="display: none;"></span></div><div class="ace_gutter-cell " style="height: 18px; top: 90px;">6<span style="display: none;"></span></div><div class="ace_gutter-cell " style="height: 18px; top: 108px;">7<span style="display: none;"></span></div><div class="ace_gutter-cell " style="height: 18px; top: 126px;">8<span style="display: none;"></span></div><div class="ace_gutter-cell " style="height: 18px; top: 144px;">9<span style="display: none;"></span></div><div class="ace_gutter-cell " style="height: 18px; top: 162px;">10<span style="display: none;"></span></div><div class="ace_gutter-cell " style="height: 18px; top: 180px;">11<span style="display: none;"></span><