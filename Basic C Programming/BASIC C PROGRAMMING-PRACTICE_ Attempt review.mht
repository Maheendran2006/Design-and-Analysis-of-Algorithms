int main(){
    int a;
    scanf("%d",&amp;a);
    int isprime=1;
    if(a&lt;=1){
        printf("No Prime");
    }
    else{
        for(int i=2;i&lt;=a/2;i++){
            if(a%i==0){
                isprime=0;
                break;
            }
        }
        if(isprime){
            printf("Prime");
        }
        else{
            printf("No Prime");
